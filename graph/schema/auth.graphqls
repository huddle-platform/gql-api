type LoginResponse{
    jwt: String
    refreshToken: String
    error: String
}

type EmailRequestResponse{
    sent: Boolean!
    message: String!
}

type Auth{
    login(username: String!, password: String!):LoginResponse
    requestNewPassword(email:String!):EmailRequestResponse
    refreshToken(refreshToken: String!):LoginResponse
    createAccount(email:String!, username: String!):User!
}
extend type Query{
    auth: Auth!
}